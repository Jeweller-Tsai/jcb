<h2>Show transaction</h2>

<ul>

  <li>
    <strong>User:</strong>
    <%= @transaction.user_id %>
  </li>

  <li>
    <strong>Recipient:</strong>
    <%= @transaction.recipient_id %>
  </li>

  <li>
    <strong>Reason:</strong>
    <%= @transaction.reason %>
  </li>

</ul>

<%= link "Edit", to: transaction_path(@conn, :edit, @transaction) %>
<%= link "Back", to: transaction_path(@conn, :index) %>
